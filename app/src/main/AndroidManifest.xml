<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sample.ml_ai">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ml_ai">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VisionActivity"/>
        <activity android:name=".TextRecognitionActivity"/>
        <activity android:name=".DetectFacesActivity"/>
        <activity android:name=".BarCodeActivity"/>
        <activity android:name=".IdentifyLangActivity"/>
        <activity android:name=".TranslateActivity"/>

        <meta-data
            android:name="com.google.mlkit.vision.DEPENDENCIES"
            android:value="ocr,face,barcode" />
    </application>

</manifest>